name: "C3D-ResNet18-Sports1M"
layer {
  name: "data"
  type: "VideoData"
  top: "data"
  top: "label"
  transform_param {
    crop_size: 112
    mean_value: 128
    mirror: false
  }
  video_data_param {
    source: "ucf101_video_frame_01.list"
    root_folder: "/"
    new_length: 16
    sampling_rate: 1
    new_height: 128
    new_width: 171
    use_image: true
    show_data: false
    use_temporal_jitter: false
    batch_size: 30
    shuffle: false
  }
}

# ----- conv1 layers -----
layer {
  bottom: "data"
  top: "conv1"
  name: "conv1"
  type: "Convolution3D"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  convolution3d_param {
    num_output: 64
    kernel_size: 7
    kernel_depth: 3
    pad: 3
    temporal_pad: 1
    stride: 2
    temporal_stride: 1
    bias_term: false
    weight_filler {
      type: "msra"
      variance_norm: FAN_OUT
    }
  }
}

layer {
  bottom: "conv1"
  top: "conv1"
  name: "bn_conv1"
  type: "BatchNorm"
  batch_norm_param {
    use_global_stats: true
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
}

layer {
  bottom: "conv1"
  top: "conv1"
  name: "scale_conv1"
  type: "Scale"
  param {
    lr_mult: 1
    decay_mult: 0
  }
  param {
    lr_mult: 1
    decay_mult: 0
